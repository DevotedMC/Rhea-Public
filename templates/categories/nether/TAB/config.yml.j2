# Detailed explanation of all options available at https://github.com/NEZNAMY/TAB/wiki/config.yml

change-nametag-prefix-suffix: false
change-tablist-prefix-suffix: false
enable-header-footer: true
header:
  - '&r&4&lDevoted Hell&r - &6{{ shard.nice_name }}&r - Hardcore Nether Survival'
  - '-------------------------------------'
footer:
  - '-------------------------------------'
  - '&4&lAdmins:'
  - '&fRelaxer &4* &fMaxopoly &4* &fProgrammerDan'
  - '&fDiet_Cola &4* &fMCSPenguin'
  - '&4Join us at:'
  - '&fdiscord.gg/devoted'
placeholderapi-refresh-intervals:
  default-refresh-interval: 100
  server:
    '%server_uptime%': 1000
    '%server_tps_1_colored%': 1000
  player:
    '%vault_prefix%': 1000
    '%player_health%': 200
    '%player_ping%': 1000
  relational:
    '%rel_factionsuuid_relation_color%': 500
placeholders:
  afk-yes: ' &4*&4&lAFK&4*&r'
  time-format: '[HH:mm:ss / h:mm a]'
  time-offset: 0.0
  afk-no: ''
  date-format: dd.MM.yyyy
  remove-strings:
    - '[] '
    - '< > '
enable-collision: false
group-sorting-priority-list:
  - Owner
  - Admin
  - Mod
  - Helper
  - Builder
  - Premium
  - Player
  - default
revert-collision-rule-in-worlds:
  - revertedworld
use-primary-group: false
primary-group-finding-list:
  - Owner
  - Admin
  - Helper
  - default
assign-groups-by-permissions: false
disable-features-in-worlds:
  yellow-number:
    - disabledworld
  belowname:
    - disabledworld
  header-footer:
    - disabledworld
do-not-move-spectators: false
classic-vanilla-belowname:
  number: '%health%'
  text: Health
  enabled: false
yellow-number-in-tablist: ''
fix-pet-names: false
per-world-playerlist:
  enabled: false
