Blacksmith:
  fullname: Blacksmith
  shortname: Bl
  description: Earns money crafting, smelting and enchanting armour & weapons.
  ChatColour: AQUA
  chat-display: full
  max-level: 666
  leveling-progression-equation: 10*(joblevel*joblevel)+(joblevel*joblevel*4)
  income-progression-equation: baseincome+(baseincome*(joblevel-1)*0.01)-((baseincome+(baseincome*(joblevel-1)*0.01))
    * ((numjobs-1)*0.05))
  points-progression-equation: basepoints
  experience-progression-equation: baseexperience
  rejoinCooldown: 10
  maxDailyQuests: 1
  Quests:
    '1':
      Name: Harry the wizard
      Objectives:
      - Enchant;diamond_sword;1
      - Enchant;ARROW_DAMAGE;1
      RewardCommands:
      - jobs editpoints add [playerName] 100
      - jobs exp add [playerName] 100
      - msg [playerName] Completed quest!
      RewardDesc:
      - Enchant some things and
      - Get 100 points & exp for this!
    '2':
      Name: War never ends
      Objectives:
      - Craft;iron_chestplate;2
      - Craft;gold_leggings;2
      RewardCommands:
      - jobs editpoints add [playerName] 100
      - jobs exp add [playerName] 100
      - msg [playerName] Completed quest!
      RewardDesc:
      - Craft weapons and
      - Get 100 points & exp for this!
    '3':
      Name: That old smell
      Objectives:
      - Smelt;netherite_scrap;1
      RewardCommands:
      - jobs editpoints add [playerName] 100
      - jobs exp add [playerName] 100
      - msg [playerName] Completed quest!
      RewardDesc:
      - Smelt some Ancient Debris and
      - Get 100 points & exp for this!
